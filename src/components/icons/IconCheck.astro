---
interface Props {
	size?: number;
	class?: string;
}

const { size = 24, class: className = '' } = Astro.props;
---

<svg
	width={size}
	height={size}
	viewBox="0 0 24 24"
	fill="none"
	stroke="currentColor"
	stroke-width="2"
	stroke-linecap="round"
	stroke-linejoin="round"
	class={className}
	aria-hidden="true"
>
	<polyline points="20 6 9 17 4 12"></polyline>
</svg>
